-
    var skills={
    Frontend:['HTML5','CSS3','JavaScript & jQuery'],//'WPF'],
    Backend:['PHP & Laravel','mySQL','Node.js & npm','Mongo.db','C#','SQL Server'],
    Workflow:['Git','Gulp','Webpack','Artisan']
    }

.c-skills
    each skillGroup, key in skills
        .c-skills-group__title= key
        ul.c-skills-list
            each value in skillGroup
                li.c-skills-list__item
                    .c-skill
                        svg(viewBox="0 0 40 40" preserveAspectRatio="none" stroke-alignment="inner").c-skill__circle.c-skill__circle_inner
                            circle(cx="20" cy="20" r="16")
                        svg(stroke-dasharray="65 100" viewBox="0 0 40 40" preserveAspectRatio="none" stroke-alignment="inner").c-skill__circle.c-skill__circle_outer
                            circle(cx="20" cy="20" r="16")
                        .c-skill__title= value